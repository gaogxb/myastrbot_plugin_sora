{
    "api_url": {
        "description": "API的请求地址",
        "type": "string",
        "hint": "指定视频生成服务的API接口地址",
        "default": "https://grsai.dakka.com.cn/v1/video/sora-video",
        "obvious_hint": false
    },
    "api_key": {
        "description": "API密钥",
        "type": "string",
        "hint": "请填写你的API服务商提供的密钥，代码会自动添加'Bearer '前缀",
        "default": "",
        "obvious_hint": false
    },
    "model": {
        "description": "视频生成模型名称",
        "type": "string",
        "hint": "指定要使用的视频生成模型，支持: sora-2",
        "default": "sora-2",
        "obvious_hint": false
    },
    "aspectRatio": {
        "description": "输出视频比例",
        "type": "string",
        "hint": "支持的比例: 9:16, 16:9",
        "default": "16:9",
        "obvious_hint": false
    },
    "duration": {
        "description": "视频时长(秒)",
        "type": "int",
        "hint": "支持的时长: 10, 15",
        "default": 10,
        "obvious_hint": false
    },
    "size": {
        "description": "视频清晰度",
        "type": "string",
        "hint": "支持的清晰度: small, large",
        "default": "small",
        "obvious_hint": false
    },
    "shutProgress": {
        "description": "关闭进度回复",
        "type": "string",
        "hint": "关闭进度回复，直接回复最终结果",
        "default": "false",
        "obvious_hint": "false"
    },
    "banana_api_url": {
        "description": "Banana API的请求地址",
        "type": "string",
        "hint": "指定图片生成服务的API接口地址",
        "default": "https://grsai.dakka.com.cn/v1/draw/nano-banana",
        "obvious_hint": false
    },
    "banana_model": {
        "description": "Banana图片生成模型名称",
        "type": "string",
        "hint": "指定要使用的图片生成模型，支持: nano-banana-fast, nano-banana, nano-banana-pro, nano-banana-pro-vt",
        "default": "nano-banana-fast",
        "obvious_hint": false
    },
    "banana_image_size": {
        "description": "Banana图片大小",
        "type": "string",
        "hint": "输出图片大小，支持: 1K, 2K, 4K（仅nano-banana-pro支持）",
        "default": "1K",
        "obvious_hint": false
    }
}
